  item_descs    String   @default("") @db.VarChar(250)
  kbki_cd       String?  @default("") @db.VarChar(50)
  catalog_no    String?  @db.VarChar(20)
  brand_name    String?  @default("") @db.VarChar(100)
  uom_cd        String?  @default("") @db.VarChar(10)
  isShowed      Boolean  @default(true)
  remarks       String?  @default("") @db.VarChar(100)
  ecatalog_url  String?  @default("") @db.VarChar(150)
  seo_title     String?  @default("") @db.VarChar(250)
  sni_cd        String?  @default("") @db.VarChar(50)
  selling_price Decimal? @default(0) @db.Decimal(9, 0)
  isMaterial    Boolean  @default(false)
  imgUrl1       String?  @default("") @db.VarChar(250)
  imgUrl2       String?  @default("") @db.VarChar(250)
  imgUrl3       String?  @default("") @db.VarChar(250)
  imgUrl4       String?  @default("") @db.VarChar(250)
